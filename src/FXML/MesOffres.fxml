               </image>
            </ImageView>
            <ImageView fx:id="ajouter" fitHeight="50.0" fitWidth="50.0" layoutX="728.0" layoutY="46.0" onMouseClicked="#toAddOffer" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Images/plus.png" />
               </image>
            </ImageView>
            <ImageView fx:id="modifierOffer" fitHeight="51.0" fitWidth="50.0" layoutX="644.0" layoutY="47.0" onMouseClicked="#toChoisirOffer" pickOnBounds="true" preserveRatio="true">
