                  <TableColumn fx:id="NbrPlaceOffer" minWidth="0.0" prefWidth="91.0" text="Place" />
                  <TableColumn fx:id="PrixOffer" minWidth="0.0" prefWidth="149.0" text="Prix" />
               </columns>
            </TableView>
            <ImageView fitHeight="51.0" fitWidth="51.0" layoutX="14.0" layoutY="46.0" onMouseClicked="#afficherMesOffres" pickOnBounds="true" preserveRatio="true">
